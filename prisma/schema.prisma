generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model news {
  id         Int     @id @default(autoincrement())
  headline   String? @unique
  date       String?
  region     String?
  scraped_at String?
  label      String?
  score      Float?
}
